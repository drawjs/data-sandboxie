{
  "include": ["src/**/*", "../Draw/src/**/*"],
  "exclude": ["node_modules", "src/__tmp__"],
  "compilerOptions": {
    "module": "es6",
    // "moduleResolution": "node",
    "target": "es5",
    "allowJs": true,
    "outDir": "./build/",
    "sourceMap": true,
    "lib": ["dom", "es2016"],
    "typeRoots": ["../Draw/src/typings/**/*", "src/typings/**"],
    "allowSyntheticDefaultImports": true
  }
}
